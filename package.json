{
  "name": "illumine-bible-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:production": "NODE_ENV=production npm run build",
    "build:analyze": "npm run build -- --mode analyze",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "test:unit": "vitest",
    "test:unit:run": "vitest --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit:run && npm run test:e2e",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint:fix": "eslint . --fix",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "deploy:preview": "npm run build && npm run preview:production",
    "clean": "rm -rf dist node_modules/.vite",
    "postinstall": "npm run security:audit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "dexie": "^4.0.11",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "zod": "^4.0.17"
  },
  "optionalDependencies": {
    "@sentry/browser": "^7.120.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/dexie": "^1.3.32",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-pwa": "^1.0.2",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  }
}
